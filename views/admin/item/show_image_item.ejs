<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <div class="row">
    <% for(let x = 0; x < item.imageId.length; x++){ %>
    <div class="col-md-4">
      <!-- DataTales Example -->
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img
            src="http://adminbwa-staycation.herokuapp.com/<%= item.imageId[x].imageUrl %>"
            alt="image"
            style="width: 100%; display: block"
          />
        </div>
      </div>
    </div>
    <% } %>
    <!-- End DataTales Example -->
  </div>
  <a href="/admin/item" class="btn btn-warning">back</a>
</div>
